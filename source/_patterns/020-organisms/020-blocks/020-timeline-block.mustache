{{#timeline-block}}
    <div class="{{#timelineBlockClasses}}{{timelineBlockClasses}} {{/timelineBlockClasses}}o-sections{{#timelineBlockClasses1}} {{timelineBlockClasses1}}{{/timelineBlockClasses1}}">
        <div class="{{#timelineBlockExtraClasses}}{{timelineBlockExtraClasses}} {{/timelineBlockExtraClasses}}t-1 u-zone-space c-content-block{{#timelineBlockExtraClasses1}} {{timelineBlockExtraClasses1}}{{/timelineBlockExtraClasses1}}">
            <div class="c-content-block__header">
				{{> atoms-section-header}}
            </div>
            <div class="c-content-block__main">
                <div class="o-sections">
                    <div class="o-sections__section">
                        <div class="o-sections__section">
							{{> atoms-paragraph}}
                        </div>
                    </div>
                    <div class="o-sections__section u-space-1">
						{{> molecules-timeline-list}}
                    </div>
                </div>
            </div>
        </div>
        <div class="u-space-1-sm@large o-sections@small o-tabular@large o-tabular-bordered@large">
            <div class="t-1@large o-sections@small__section o-sections@small o-tabular-bordered@large__row u-size-2@large o-tabular@large__cell">
                <div class="{{#timelineBlockExtraClasses}}{{timelineBlockExtraClasses}} {{/timelineBlockExtraClasses}}o-sections@small__section t-1 u-zone-space c-content-block{{#timelineBlockExtraClasses1}} {{timelineBlockExtraClasses1}}{{/timelineBlockExtraClasses1}}">
                    <div class="c-content-block__header">
						{{> atoms-section-header}}
                    </div>
                    <div class="c-content-block__main">
						{{> atoms-paragraph}}
                    </div>
                </div>
            </div>
            <div class="t-1@large o-sections@small__section o-sections@small-medium o-sections@small-medium__section o-tabular-bordered@large__row u-size-2@large o-tabular@large__cell">
                <div class="{{#timelineBlockExtraClasses}}{{timelineBlockExtraClasses}} {{/timelineBlockExtraClasses}}t-1 u-zone-space c-content-block{{#timelineBlockExtraClasses1}} {{timelineBlockExtraClasses1}}{{/timelineBlockExtraClasses1}}">
                    <div class="c-content-block__header">
						{{> atoms-section-header}}
                    </div>
                    <div class="c-content-block__main">
						{{> atoms-paragraph}}
                    </div>
                </div>
            </div>
        </div>
    </div>
{{/timeline-block}}
